{
  "name": "goodreads-bookshelf-api",
  "version": "1.0.0",
  "description": "Unofficial Goodreads API for getting any user's bookshelf details",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "ts-node src/index.ts",
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "goodreads",
    "api",
    "bookshelf",
    "books",
    "nodejs",
    "typescript",
    "javascript",
    "rss"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "author": "tnmyk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnmyk/goodreads-bookshelf-api"
  },
  "homepage": "https://github.com/tnmyk/goodreads-shelf-apis#readme",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "rss-parser": "^3.12.0"
  }
}
